{
  "name": "ap-5b-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "docker-dev": "docker compose -f docker-compose-dev.yml up",
    "docker-build": "docker compose -f docker-compose-dev.yml up --build",
    "docker-prod": "docker-compose -f docker-compose-prod.yml up -d --build",
    "docker-prod-down": "docker-compose -f docker-compose-prod.yml down",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "DEBUG=myapp:* nodemon --watch 'src/**/*.ts' --exec 'tsx' src/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prepare": "if [ \"$NODE_ENV\" = \"development\" ]; then husky install; fi",
    "test": "jest --runInBand"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "module",
    "url": "git+https://github.com/FinzoYFunzo/ap-5b-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/FinzoYFunzo/ap-5b-backend/issues"
  },
  "homepage": "https://github.com/FinzoYFunzo/ap-5b-backend#readme",
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.7.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "prisma": "^6.16.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  }
}
